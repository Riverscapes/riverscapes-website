{
  "name": "@riverscapes/website",
  "description": "Riverscapes website powered by Gatsby and Markdown",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Matt Reimer <matt@northarrowresearch.com>",
  "dependencies": {
    "@riverscapes/gatsby-theme": "^0.1.20",
    "gatsby": "^5.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "gatsby": "^5.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "upgrade:theme": "yarn upgrade @riverscapes/gatsby-theme",
    "start": "NODE_OPTIONS=\"--max-old-space-size=8192\" gatsby develop -p 8000",
    "start:clean": "gatsby clean; NODE_OPTIONS=\"--max-old-space-size=8192\" gatsby develop -p 8000",
    "clean": "gatsby clean",
    "serve": "gatsby serve --prefix-paths",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  }
}
